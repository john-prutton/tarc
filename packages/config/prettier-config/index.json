{
  "semi": false,
  "trailingComma": "none",

  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],

  "importOrder": [
    "^next($|/.*)$",
    "^react($|/.*)$",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/.*$",
    "",
    "^[.]",
    "",
    ".css$"
  ]
}
